<h1>Edit Tweet</h1>
<%= @tweet.content %>

<form action="/tweets/<%= @tweet.id %>" method="POST">
  <input type="hidden" name="_method" id="hidden" value="patch" />
  <textarea name="content"></textarea>
  
  <input type="submit" value="submit" />
</form>

<form action="/tweets/:id/delete" method="POST">
  <input type="hidden" name="_method" id="hidden" value="delete" />
  <button type="submit" name="Delete Tweet">Delete Tweet</button>
</form>